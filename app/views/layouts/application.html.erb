<!DOCTYPE html>
<html>
  <head>
    <title>AssessmentWeek2</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render "/layouts/menu"%>
    <div class="container">
      <div class="row">
        <%= flash[:update_success] %>
        <%= flash[:update_error] %>
        <%= flash[:alert] %>
        <%= flash[:notice] %>
        <%= yield %>
      </div>
    </div>
  </body>
</html>
