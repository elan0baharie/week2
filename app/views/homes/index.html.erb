<% if !current_user %>
  <div class="locked">
    <div class="front-button">
      <%= link_to "Sign In", new_user_session_path %>
    </div>
    <div class="">
      <%= link_to "Sign Up", new_user_registration_path %>
    </div>
  </div>
  <% else %>
    <%= current_user.email %>
    <%= current_user.id %>
    <h3>You logged in!</h3>
    <%= link_to "Users", users_path, method: :delete %>
  <% end %>
<div class="well">
  <h2>Welcome times</h2>


</div>
